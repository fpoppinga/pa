%% load file
bcok = audioread('signal_single_chirp.wav');
silent = audioread('silence_single_chirp.wav');
noise1 = audioread('noise_1.wav');
noise2 = audioread('noise_2.wav');
%% plot
figure();
spectrogram(bcok(:, 2),128, 120, 256, 44100, 'yaxis');

%% plot noises
figure();
subplot(3, 1, 1); spectrogram(silent(:, 2),128, 120, 256, 44100, 'yaxis');
subplot(3, 1, 2); spectrogram(noise1(:, 2),128, 120, 256, 44100, 'yaxis');
subplot(3, 1, 3); spectrogram(noise2(:, 2),128, 120, 256, 44100, 'yaxis');
